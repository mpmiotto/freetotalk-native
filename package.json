{
  "name": "imfree-app",
  "version": "1.0.3",
  "main": "index.js",
  "metro": "metro.config.cjs",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "update-build-numbers": "node update-build-numbers.js",
    "build:ios": "npm run update-build-numbers && eas build --platform ios --profile preview",
    "build:android": "npm run update-build-numbers && eas build --platform android --profile preview",
    "submit:ios": "eas submit --platform ios --profile preview",
    "update": "eas update --channel production",
    "update:rc": "eas update --branch release-candidate"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "expo": "53.0.22",
    "expo-constants": "~17.1.7",
    "expo-contacts": "~14.2.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-notifications": "~0.31.4",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-webview": "13.13.5",
    "expo-build-properties": "~0.14.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
